<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:sp="http://schemas.android.com/com.lianbi.mezone.b.fragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

   <!-- <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >-->
    <cn.com.hgh.view.AbPullToRefreshView
        android:id="@+id/act_addmembers_abpulltorefreshview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="10dp"
        android:background="@color/white"
        android:orientation="vertical"
        android:visibility="visible">

        <cn.com.hgh.view.SwipeListView
            android:id="@+id/act_addmembers_listview"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#fff4f7f9"
            android:cacheColorHint="#00000000"
            android:divider="#dddbdb"
            android:dividerHeight="1dp"
            android:listSelector="@color/transparent"
            sp:right_width="120dp"
            android:scrollbars="none">
        </cn.com.hgh.view.SwipeListView>
    </cn.com.hgh.view.AbPullToRefreshView>
        <ImageView
            android:visibility="gone"
            android:id="@+id/fm_orederfragment_iv_empty"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:src="@mipmap/iv_empty"
            android:scaleType="fitXY"
             />
<!--</FrameLayout>-->

   
</RelativeLayout>